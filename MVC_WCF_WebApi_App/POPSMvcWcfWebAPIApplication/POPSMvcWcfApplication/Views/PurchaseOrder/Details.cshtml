@model IEnumerable< POPSMvcWcfApplication.ServiceReference1.PurchaseOrderDetails >

@{
    ViewBag.Title = "Order Details";
}
<div>
    <h3>PurchaseOrderDetails</h3>
    <hr />
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PONO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ITCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Qty)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.PONO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ITCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Qty)
                </td>
            </tr>
        }

    </table>
</div>
<p>
    @Html.ActionLink("Back to Order List", "Index")
</p>

@model IEnumerable<POPSMvcWcfApplication.ServiceReference1.PurchaseOrderMaster>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Place New Order", "Order")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PODate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PONO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SUPLNO)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PODate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PONO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SUPLNO)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id = item.PONO.Trim() }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PONO.Trim() })
        </td>
    </tr>
}

</table>
